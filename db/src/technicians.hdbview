VIEW H2O_TECHNICIANS AS
SELECT "TechID" AS TECHID,
  "TechFirstname" AS TECHFIRSTNAME,
  "TechLastname" AS TECHLASTNAME,
  "TechRole" AS TECHROLE,
  "TechEmail" AS TECHEMAIL,
  "TechMobile" AS TECHMOBILE,
  "CreatedBy" AS CREATEDBY,
  "CreatedTimestamp" AS CREATEDTIMESTAMP
FROM "H2O_TECHNICIANS_SYNONYM";

